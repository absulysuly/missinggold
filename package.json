    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "db:deploy": "prisma migrate deploy",
    "db:setup": "npm run db:deploy && npm run db:seed",
    "db:studio": "prisma studio",
    "clean": "rimraf .next out",
    "i18n:check": "node scripts/validate-i18n.js",
    "validate:translations": "node scripts/validate-translations.js",
    "validate:translations:ci": "node scripts/validate-translations.js --fail-fast",
    "import:events:csv": "tsx scripts/csv-to-events-json.ts",
    "test": "echo \"No tests specified\" && exit 0",
