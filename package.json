    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "next-intl": "^4.3.9",
    "next-pwa": "^5.6.0",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
