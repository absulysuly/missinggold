    "qa:test:headed": "playwright test --headed",
    "qa:a11y": "playwright test tests/08_accessibility.spec.ts",
    "qa:perf": "node scripts/run_lhci.mjs",
    "prebuild": "npm run lint",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.1",
