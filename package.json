{
  "name": "eventra-venues",
  "version": "2.0.0",
  "private": true,
  "description": "Complete travel and entertainment platform for Iraq & Kurdistan - Events, Hotels, Restaurants, Activities",
  "author": "Your Name",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "typescript",
    "event-management",
    "hotels",
    "restaurants",
    "activities",
